{
    "builds": [{
        "src": "whatbytes/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "50mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/staticfiles/(.*)",     
            "dest": "/staticfiles/$1" 
        },
        {
            "src": "/(.*)",
            "dest": "whatbytes/wsgi.py"
        }
    ]
}